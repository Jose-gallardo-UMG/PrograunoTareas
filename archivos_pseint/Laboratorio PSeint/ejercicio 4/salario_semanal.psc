Algoritmo salario_semanal
	Escribir 'Ingrese el nombre del empleado: ';
	Leer empleado;
	Escribir 'Ingrese las horas laboradas entre semana: ';
	Leer horas;
	Escribir 'Ingrese el pago por hora del empleado ', empleado;
	Leer pago;
	salario <- horas*pago;
	Escribir 'El salario semanal del empleado ', empleado, ' es de Q', salario;
FinAlgoritmo
