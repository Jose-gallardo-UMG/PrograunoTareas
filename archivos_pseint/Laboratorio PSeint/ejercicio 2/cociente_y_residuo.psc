Algoritmo cociente_y_residuo
	Escribir 'ingrese numero dividendo';
	Leer dividendo;
	Escribir 'ingrese divisor';
	Leer divisor;
	cociente <- dividendo/divisor;
	residuo <- dividendo MOD divisor;
	Escribir 'El cociente de la division es:', cociente;
	Escribir 'El residuo de la division es: ', residuo;
FinAlgoritmo
